			  SKIN.NAME itemBundleSkinName 
			  FROM ITEM_BUNDLE ,ITEM_BUNDLE_TYPE,ITEM_BUNDLE_I18N,SKIN 
			  WHERE ITEM_BUNDLE.SID = 10
			  AND ITEM_BUNDLE_I18N.ITEM_BUNDLE_SID = ITEM_BUNDLE.SID
